{
  "name": "sample-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "xxpreinstall": "npx npm-force-resolutions",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "nth-check": ">=2.1.1",
    "css-select": ">=5.1.0",
    "parse-path": "^5.0.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.1.2",
    "axios": "^1.7.4",
    "bootstrap": "^5.3.3",
    "bootstrap-vue": "^1.5.1",
    "core-js": "^3.19.3",
    "nuxt": "3.12.4",
    "parse-path": "^5.0.0",
    "parse-url": "^8.1.0",
    "vue": "^3.0.0",
    "vue-server-renderer": "^2.7.16",
    "vue-template-compiler": "^0.1.0",
    "webpack": "^5.76.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
